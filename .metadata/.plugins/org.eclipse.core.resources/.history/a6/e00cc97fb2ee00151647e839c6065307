
public class ThisKeyWord {
	public static void main(String[] args)
	{
		Person2 per = new Person2();
		per.pid = 1;
		per.name = "tom";
		per.age = 20;
		per.display();
		
		
	}

}

class Person2
{
	int pid;
	String name;
	int age;
	
	void display()
	{
		msg = pid+","+name+","+age;
		System.out.println(msg);
	}
}