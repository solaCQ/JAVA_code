/**
 * 
 * @author chenqiang
 *构造函数，  constructor
 */
public class Constructor {

	public static void main(String[] args)
	{
		Person1 per1 = new Person1();
		
	}
}	



class Person1
	{
	
		int pid;
		String name;
		int age;
	 
		//构造函数与类同名，大小写相同
		public Person1()
		{
			//实例化变量
			System.out.println("Person");
		}
		
		
		
		
	}
	
	
	

